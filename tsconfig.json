{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@assets/*": ["src/assets/*"],
			"@layouts/*": ["src/layouts/*"],
			"@scripts": ["src/scripts/index"],
			"@styles/*": ["src/styles/*"],
			"@public/*": ["public/*"]
		}
	}
}
